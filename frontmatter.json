{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.content.pageFolders": [
    {
      "title": "Blog Posts",
      "path": "[[workspace]]/content/blog",
      "previewPath": "blog"
    },
    {
      "title": "Services",
      "path": "[[workspace]]/content/services",
      "previewPath": "services"
    }
  ],
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "fields": [
        {
          "title": "draft",
          "name": "draft",
          "type": "draft"
        },
        {
          "title": "title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "date",
          "name": "date",
          "type": "datetime"
        },
        {
          "title": "preview",
          "name": "preview",
          "type": "string"
        },
        {
          "title": "tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "categories",
          "name": "categories",
          "type": "categories"
        }
      ]
    }
  ],
  "frontMatter.content.publicFolder": "static",
  "frontMatter.framework.id": "hugo",
  "frontMatter.preview.host": "http://localhost:1313"
}